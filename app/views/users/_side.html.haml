.myside
   %nav.myside--top
      %li.myside--top__list__active
         = link_to "マイページ", user_path(current_user.id), class: 'myside--top__list__box'
         = icon "fas",'chevron-right', class: 'icon'
      %li.myside--top__list
         = link_to "お知らせ", notification_user_path(current_user.id), class: "myside--top__list__box"
         = icon "fas",'chevron-right', class: 'icon'
      %li.myside--top__list
         = link_to "やることリスト", todo_user_path(current_user.id), class: "myside--top__list__box"
         = icon "fas",'chevron-right', class: 'icon'
      %li.myside--top__list
         = link_to "いいね！一覧", likes_user_path(current_user.id), class: "myside--top__list__box"
         = icon "fas",'chevron-right', class: 'icon'
      %li.myside--top__list
         = link_to "出品する", new_good_path, class: 'myside--top__list__box'
         = icon "fas",'chevron-right', class: 'icon'
      %li.myside--top__list
         = link_to "出品した商品 出品中", items_user_path(current_user.id), class: "myside--top__list__box"
         = icon "fas",'chevron-right', class: 'icon'
      %li.myside--top__list
         = link_to "出品した商品 取引中", dealing_user_path(current_user.id), class: "myside--top__list__box"
         = icon "fas",'chevron-right', class: 'icon'
      %li.myside--top__list
         = link_to "出品した商品 売却済み", bought_user_path(current_user.id), class: "myside--top__list__box"
         = icon "fas",'chevron-right', class: 'icon'
      %li.myside--top__list
         = link_to "購入した商品 取引中", buying_user_path(current_user.id), class: "myside--top__list__box"
         = icon "fas",'chevron-right', class: 'icon'
      %li.myside--top__list
         = link_to "購入した商品 過去の取引", todo_user_path(current_user.id), class: "myside--top__list__box"
         = icon "fas",'chevron-right', class: 'icon'
      %li.myside--top__list
         = link_to "ニュース一覧", news_user_path(current_user.id), class: "myside--top__list__box"
         = icon "fas",'chevron-right', class: 'icon'
      %li.myside--top__list
         = link_to "ガイド", guide_user_path(current_user.id), class: "myside--top__list__box"
         = icon "fas",'chevron-right', class: 'icon'
      %li.myside--top__list
         = link_to "お問い合わせ", contact_user_path(current_user.id), class: "myside--top__list__box"
         = icon "fas",'chevron-right', class: 'icon'
   %h3.myside--while
      メルペイ
   %ul.myside--while__list
      %li.myside--while__list__tag
         売り上げ・振込申請
         = icon "fas",'chevron-right', class: 'icon'
      %li.myside--while__list__tag
         ポイント申請
         = icon "fas",'chevron-right', class: 'icon'
   %h3.myside--under
      設定
   %ul.myside--under__list
      = link_to "プロフィール", profile_user_path(current_user.id), class: "myside--top__list__box"
      = icon "fas",'chevron-right', class: 'icon'
   %ul.myside--under__list
      = link_to "発送先・お届け先住所変更", address_user_path(current_user.id), class: "myside--top__list__box"
      = icon "fas",'chevron-right', class: 'icon'
   %ul.myside--under__list
      = link_to "支払い方法", payment_user_path(current_user.id), class: "myside--top__list__box"
      = icon "fas",'chevron-right', class: 'icon'
   %ul.myside--under__list
      = link_to "メール/パスワード", email_user_path(current_user.id), class: "myside--top__list__box"
      = icon "fas",'chevron-right', class: 'icon'
   %ul.myside--under__list
      = link_to "本人情報", identification_user_path(current_user.id), class: "myside--top__list__box"
      = icon "fas",'chevron-right', class: 'icon'
   %ul.myside--under__list
      = link_to "電話番号の確認", phone_user_path(current_user.id), class: "myside--top__list__box"
      = icon "fas",'chevron-right', class: 'icon'
   %ul.myside--under__list
      = link_to "ログアウト", logout_user_path(current_user.id), class: "myside--top__list__box"
      = icon "fas",'chevron-right', class: 'icon'